			"minimum": 1
		},
		"modified_on": {
			"type": "integer",
			"minimum": 1
		},
		"user_id": {
			"type": "integer",
			"minimum": 1
		},
		"name": {
			"type": "string",
			"minLength": 1
		},
		"host_type": {
			"type": "string",
			"pattern": "^proxy|redirect|dead|stream$"
		},
		"template": {
			"type": "string",
			"minLength": 20
		}
