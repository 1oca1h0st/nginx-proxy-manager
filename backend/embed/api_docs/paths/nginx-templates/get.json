{
	"operationId": "getNginxTemplates",
	"summary": "Get a list of Nginx Templates",
	"tags": ["Nginx Templates"],
	"parameters": [
		{
			"in": "query",
			"name": "offset",
			"schema": {
				"type": "number"
			},
			"description": "The pagination row offset, default 0",
			"example": 0
		},
		{
			"in": "query",
			"name": "limit",
			"schema": {
				"type": "number"
			},
			"description": "The pagination row limit, default 10",
			"example": 10
		},
		{
			"in": "query",
			"name": "sort",
			"schema": {
				"type": "string"
			},
