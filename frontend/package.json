		"@tanstack/react-query-devtools": "^4.29.25",
		"ajv": "^8.12.0",
		"chakra-react-select": "^4.6.0",
		"classnames": "^2.3.2",
		"country-flag-icons": "^1.5.7",
		"date-fns": "2.30.0",
		"formik": "^2.4.2",
		"framer-motion": "^10.13.0",
		"humps": "^2.0.1",
		"lodash": "4.17.21",
		"moment": "2.29.4",
		"query-string": "8.1.0",
		"react": "^18.2.0",
		"react-async": "10.0.1",
		"react-dom": "18.2.0",
		"react-focus-lock": "^2.9.5",
		"react-icons": "^4.10.1",
		"react-intl": "^6.4.4",
		"react-markdown": "^8.0.7",
		"react-router-dom": "^6.14.2",
		"react-syntax-highlighter": "^15.5.0",
		"react-table": "7.8.0",
		"rooks": "7.14.1",
		"tmp": "^0.2.1"
	},
	"devDependencies": {
		"@formatjs/cli": "^6.1.3",
